{
  "id": null,
  "uid": "shipping-api-overview",
  "title": "99minutos Shipping API",
  "tags": [
    "shipping",
    "api"
  ],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "15s",
  "time": {
    "from": "now-30m",
    "to": "now"
  },
  "templating": {
    "list": []
  },
  "annotations": {
    "list": []
  },
  "panels": [
    {
      "id": 1,
      "title": "HTTP Throughput (req/s)",
      "type": "timeseries",
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 12,
        "h": 8
      },
      "targets": [
        {
          "datasource": "Prometheus",
          "expr": "sum(rate(shipping_http_requests_total[1m])) by (method, url, code)",
          "legendFormat": "{{method}} {{url}} {{code}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "lineWidth": 2
          }
        }
      }
    },
    {
      "id": 2,
      "title": "HTTP Latency p50 / p95 / p99 (ms)",
      "type": "timeseries",
      "gridPos": {
        "x": 12,
        "y": 0,
        "w": 12,
        "h": 8
      },
      "targets": [
        {
          "datasource": "Prometheus",
          "expr": "histogram_quantile(0.50, sum(rate(shipping_http_request_duration_seconds_bucket[1m])) by (le, uri))",
          "legendFormat": "p50 {{uri}}"
        },
        {
          "datasource": "Prometheus",
          "expr": "histogram_quantile(0.95, sum(rate(shipping_http_request_duration_seconds_bucket[1m])) by (le, uri))",
          "legendFormat": "p95 {{uri}}"
        },
        {
          "datasource": "Prometheus",
          "expr": "histogram_quantile(0.99, sum(rate(shipping_http_request_duration_seconds_bucket[1m])) by (le, uri))",
          "legendFormat": "p99 {{uri}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": {
            "lineWidth": 2
          }
        }
      }
    },
    {
      "id": 3,
      "title": "HTTP Error Rate (4xx + 5xx)",
      "type": "timeseries",
      "gridPos": {
        "x": 0,
        "y": 8,
        "w": 12,
        "h": 8
      },
      "targets": [
        {
          "datasource": "Prometheus",
          "expr": "sum(rate(shipping_http_requests_total{code=~\"4..|5..\"}[1m])) by (url, code)",
          "legendFormat": "{{code}} {{url}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "color": {
            "mode": "palette-classic"
          }
        }
      }
    },
    {
      "id": 4,
      "title": "Events Processed / s",
      "type": "timeseries",
      "gridPos": {
        "x": 12,
        "y": 8,
        "w": 12,
        "h": 8
      },
      "targets": [
        {
          "datasource": "Prometheus",
          "expr": "sum(rate(shipping_events_processed_total[1m])) by (status, source)",
          "legendFormat": "{{status}} / {{source}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "custom": {
            "lineWidth": 2
          }
        }
      }
    },
    {
      "id": 5,
      "title": "Event Processing Duration p50 / p95",
      "type": "timeseries",
      "gridPos": {
        "x": 0,
        "y": 16,
        "w": 12,
        "h": 8
      },
      "targets": [
        {
          "datasource": "Prometheus",
          "expr": "histogram_quantile(0.50, sum(rate(shipping_event_processing_duration_seconds_bucket[1m])) by (le, status))",
          "legendFormat": "p50 {{status}}"
        },
        {
          "datasource": "Prometheus",
          "expr": "histogram_quantile(0.95, sum(rate(shipping_event_processing_duration_seconds_bucket[1m])) by (le, status))",
          "legendFormat": "p95 {{status}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": {
            "lineWidth": 2
          }
        }
      }
    },
    {
      "id": 6,
      "title": "Event Queue Depth per Worker",
      "type": "timeseries",
      "gridPos": {
        "x": 12,
        "y": 16,
        "w": 12,
        "h": 8
      },
      "targets": [
        {
          "datasource": "Prometheus",
          "expr": "shipping_events_queue_depth",
          "legendFormat": "worker {{worker_id}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "lineWidth": 2
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 128
              },
              {
                "color": "red",
                "value": 220
              }
            ]
          }
        }
      }
    },
    {
      "id": 7,
      "title": "Dedup Hit Rate",
      "type": "piechart",
      "gridPos": {
        "x": 0,
        "y": 24,
        "w": 8,
        "h": 8
      },
      "targets": [
        {
          "datasource": "Prometheus",
          "expr": "sum(increase(shipping_events_dedup_total[5m])) by (result)",
          "legendFormat": "{{result}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      },
      "options": {
        "pieType": "donut",
        "displayLabels": [
          "name",
          "percent"
        ]
      }
    },
    {
      "id": 8,
      "title": "Shipments Created / s",
      "type": "timeseries",
      "gridPos": {
        "x": 8,
        "y": 24,
        "w": 8,
        "h": 8
      },
      "targets": [
        {
          "datasource": "Prometheus",
          "expr": "sum(rate(shipping_shipments_created_total[1m])) by (service_type)",
          "legendFormat": "{{service_type}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "custom": {
            "lineWidth": 2
          }
        }
      }
    },
    {
      "id": 9,
      "title": "Event Processing Errors / s",
      "type": "timeseries",
      "gridPos": {
        "x": 16,
        "y": 24,
        "w": 8,
        "h": 8
      },
      "targets": [
        {
          "datasource": "Prometheus",
          "expr": "sum(rate(shipping_events_errors_total[1m])) by (reason)",
          "legendFormat": "{{reason}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "color": {
            "mode": "fixed",
            "fixedColor": "red"
          }
        }
      }
    },
    {
      "id": 10,
      "title": "Total Requests",
      "type": "stat",
      "gridPos": {
        "x": 0,
        "y": 32,
        "w": 6,
        "h": 4
      },
      "targets": [
        {
          "datasource": "Prometheus",
          "expr": "sum(shipping_http_requests_total)"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      }
    },
    {
      "id": 11,
      "title": "Total Events Processed",
      "type": "stat",
      "gridPos": {
        "x": 6,
        "y": 32,
        "w": 6,
        "h": 4
      },
      "targets": [
        {
          "datasource": "Prometheus",
          "expr": "sum(shipping_events_processed_total)"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      }
    },
    {
      "id": 12,
      "title": "Total Shipments Created",
      "type": "stat",
      "gridPos": {
        "x": 12,
        "y": 32,
        "w": 6,
        "h": 4
      },
      "targets": [
        {
          "datasource": "Prometheus",
          "expr": "sum(shipping_shipments_created_total)"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      }
    },
    {
      "id": 13,
      "title": "Total Events Errors",
      "type": "stat",
      "gridPos": {
        "x": 18,
        "y": 32,
        "w": 6,
        "h": 4
      },
      "targets": [
        {
          "datasource": "Prometheus",
          "expr": "sum(shipping_events_errors_total)"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "red",
                "value": 1
              }
            ]
          },
          "color": {
            "mode": "thresholds"
          }
        }
      }
    },
    {
      "id": 14,
      "title": "Goroutines & OS Threads",
      "description": "go_goroutines: active goroutines. Sudden spikes may indicate a goroutine leak. go_threads: OS-level threads created by the Go runtime.",
      "type": "timeseries",
      "gridPos": {
        "x": 0,
        "y": 36,
        "w": 12,
        "h": 8
      },
      "targets": [
        {
          "datasource": "Prometheus",
          "expr": "go_goroutines",
          "legendFormat": "goroutines"
        },
        {
          "datasource": "Prometheus",
          "expr": "go_threads",
          "legendFormat": "OS threads"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0,
          "custom": {
            "lineWidth": 2
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 500
              },
              {
                "color": "red",
                "value": 1000
              }
            ]
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "OS threads"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "purple"
                }
              },
              {
                "id": "custom.lineStyle",
                "value": {
                  "dash": [
                    10,
                    10
                  ],
                  "fill": "dash"
                }
              }
            ]
          }
        ]
      },
      "options": {
        "tooltip": {
          "mode": "multi"
        }
      }
    },
    {
      "id": 15,
      "title": "GC Duration (p50 / p99)",
      "description": "go_gc_duration_seconds: time the garbage collector pauses the program. High p99 values (>5ms) can impact API latency.",
      "type": "timeseries",
      "gridPos": {
        "x": 12,
        "y": 36,
        "w": 12,
        "h": 8
      },
      "targets": [
        {
          "datasource": "Prometheus",
          "expr": "histogram_quantile(0.50, rate(go_gc_duration_seconds_bucket[2m]))",
          "legendFormat": "GC p50"
        },
        {
          "datasource": "Prometheus",
          "expr": "histogram_quantile(0.99, rate(go_gc_duration_seconds_bucket[2m]))",
          "legendFormat": "GC p99"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 3,
          "custom": {
            "lineWidth": 2
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 0.005
              },
              {
                "color": "red",
                "value": 0.02
              }
            ]
          }
        }
      },
      "options": {
        "tooltip": {
          "mode": "multi"
        }
      }
    },
    {
      "id": 16,
      "type": "row",
      "title": "Recursos del Contenedor API",
      "collapsed": false,
      "gridPos": {
        "x": 0,
        "y": 44,
        "w": 24,
        "h": 1
      }
    },
    {
      "id": 17,
      "type": "timeseries",
      "title": "CPU Usage (%)",
      "description": "Uso de CPU del contenedor shipping_api",
      "gridPos": {
        "x": 0,
        "y": 45,
        "w": 8,
        "h": 8
      },
      "datasource": "Prometheus",
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "color": {
            "mode": "palette-classic"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 70
              },
              {
                "color": "red",
                "value": 90
              }
            ]
          },
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10
          }
        },
        "overrides": []
      },
      "options": {
        "tooltip": {
          "mode": "multi"
        },
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      },
      "targets": [
        {
          "datasource": "Prometheus",
          "expr": "rate(process_cpu_seconds_total{job=\"shipping_api\"}[1m]) * 100",
          "legendFormat": "CPU %",
          "refId": "A"
        }
      ]
    },
    {
      "id": 18,
      "type": "timeseries",
      "title": "RAM Usage (MB)",
      "description": "Memoria residente del proceso API (RSS via process_resident_memory_bytes)",
      "gridPos": {
        "x": 8,
        "y": 45,
        "w": 8,
        "h": 8
      },
      "datasource": "Prometheus",
      "fieldConfig": {
        "defaults": {
          "unit": "decmbytes",
          "color": {
            "mode": "palette-classic"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 256
              },
              {
                "color": "red",
                "value": 512
              }
            ]
          },
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10
          }
        },
        "overrides": []
      },
      "options": {
        "tooltip": {
          "mode": "multi"
        },
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      },
      "targets": [
        {
          "datasource": "Prometheus",
          "expr": "process_resident_memory_bytes{job=\"shipping_api\"} / 1024 / 1024",
          "legendFormat": "RSS MB",
          "refId": "A"
        }
      ]
    },
    {
      "id": 19,
      "type": "timeseries",
      "title": "Go Heap (MB)",
      "description": "Memoria heap Go: alloc = en uso, sys = reservada del SO",
      "gridPos": {
        "x": 16,
        "y": 45,
        "w": 8,
        "h": 8
      },
      "datasource": "Prometheus",
      "fieldConfig": {
        "defaults": {
          "unit": "decmbytes",
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10
          }
        },
        "overrides": []
      },
      "options": {
        "tooltip": {
          "mode": "multi"
        },
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      },
      "targets": [
        {
          "datasource": "Prometheus",
          "expr": "go_memstats_heap_alloc_bytes{job=\"shipping_api\"} / 1024 / 1024",
          "legendFormat": "Heap alloc",
          "refId": "A"
        },
        {
          "datasource": "Prometheus",
          "expr": "go_memstats_heap_sys_bytes{job=\"shipping_api\"} / 1024 / 1024",
          "legendFormat": "Heap sys",
          "refId": "B"
        }
      ]
    },
    {
      "id": 20,
      "type": "row",
      "title": "Errores de Aplicaci√≥n",
